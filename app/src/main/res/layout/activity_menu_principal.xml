<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout  xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FFFFFF"
    tools:context="mz.gerasoft.regulador_rodovia.Menu_principal"
    android:weightSum="1">


    <android.support.v7.widget.Toolbar
        android:id="@+id/toolbar2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="#2E8B57"
        android:minHeight="?attr/actionBarSize"
        android:theme="?attr/actionBarTheme"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />


  


    <ImageButton
        android:id="@+id/imageButton5"
        android:tooltipText="@string/cadastrar"
        android:contextClickable="true"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@mipmap/cadrastrar"
        android:background="#2E8B57"
        android:layout_alignTop="@+id/toolbar2"
        android:layout_alignParentStart="true" />

    <ImageButton
        android:id="@+id/imageButton6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#2E8B57"
        app:srcCompat="@mipmap/consultar"
        android:layout_alignTop="@+id/toolbar2"
        android:layout_toEndOf="@+id/imageButton5"
        android:layout_marginStart="30dp" />

    <ImageButton
        android:id="@+id/imageButton7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#2E8B57"
        android:layout_marginEnd="83dp"
        app:srcCompat="@mipmap/verartigo"
        android:layout_alignTop="@+id/toolbar2"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:id="@+id/imageButton8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#2E8B57"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/toolbar2"
        app:srcCompat="@mipmap/relatorio" />

    <ImageButton
        android:id="@+id/imageButton9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="#2E8B57"
        app:srcCompat="@mipmap/user"
        android:layout_alignTop="@+id/toolbar2"
        android:layout_centerHorizontal="true" />

    <ViewStub
        android:id="@+id/cadastrarMulta1"
        android:layout_width="400dp"
        android:inflatedId="@+id/idinflated1"
        android:layout_height="500dp"
        android:layout="@layout/activity_cadastrar_multa"
        android:layout_alignParentStart="true" />

    <ViewStub
        android:id="@+id/consultarMulta1"
        android:layout_width="wrap_content"
        android:inflatedId="@+id/idinflated2"
        android:layout_height="wrap_content"
        android:layout="@layout/activity_consultar_multa"
        android:layout_alignParentStart="true" />

    <ViewStub
        android:id="@+id/relatorio"
        android:layout_width="wrap_content"
        android:inflatedId="@+id/idinflated3"
        android:layout_height="wrap_content"
        android:layout="@layout/activity_gerar_relatorio"
        android:layout_alignParentStart="true" />

    <ViewStub
        android:id="@+id/consultarArtigo1"
        android:layout_width="wrap_content"
        android:inflatedId="@+id/idinflated3"
        android:layout_height="wrap_content"
        android:layout="@layout/activity_consultar_artigo"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_toEndOf="@+id/cadastrarMulta1"
        android:text="Cadastrar" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignStart="@+id/imageButton6"
        android:layout_alignTop="@+id/textView5"
        android:text="Consultar" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Artigo"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/imageButton7" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:text="Relatorio" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:text="Conta" />


</RelativeLayout >
